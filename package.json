{
  "name": "@johnshopkins/brand-colors",
  "version": "0.1.0",
  "description": "",
  "type": "module",
  "files": [
    "dist",
    "config/palettes.json",
    "config/web-colors.json"
  ],
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "scripts": {
    "build": "vite build --emptyOutDir",
    "test": "jest",
    "coverage": "jest --coverage",
    "format-json": "node_modules/.bin/prettier config/*.json --write --single-quote --trailing-comma all",
    "generate-palettes-json": "php ./scripts/generate-palettes.php json && npm run format-json",
    "generate-web-colors-json": "php ./scripts/generate-web-colors.php json && npm run format-json",
    "print-grades": "php ./scripts/print-grades.php",
    "eval-color-grades": "php ./scripts/eval-color-grades.php"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/johnshopkins/brand-colors.git"
  },
  "author": "Jen Wachter",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/johnshopkins/brand-colors/issues"
  },
  "homepage": "https://github.com/johnshopkins/brand-colors#readme",
  "devDependencies": {
    "@babel/preset-env": "^7.24.0",
    "babel-jest": "^29.7.0",
    "jest": "^29.7.0",
    "prettier": "^3.3.1",
    "vite": "^5.1.6"
  },
  "jest": {
    "transform": {
      "^.+\\.[t|j]sx?$": "babel-jest"
    }
  }
}
